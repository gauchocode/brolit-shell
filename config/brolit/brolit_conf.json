{
    "BROLIT_SETUP": {
        "config": [
            {
                "version": "3.10.6",
                "debug": "true",
                "skip_test": "true",
                "tmp_dir": "tmp",
                "check_packages": "true"
            }
        ]
    },
    "SERVER_CONFIG": {
        "type": "production",
        "timezone": "America/Argentina/Buenos_Aires",
        "unattended_upgrades": "disabled",
